/*CREATE TABLE DDL / SQL_COMMANDS:*/

/*FISHCOUNTER_TABLE:*/
CREATE TABLE "FISHCOUNTER_TABLE" 
   (	"IDENTCOUNTER" NUMBER GENERATED BY DEFAULT ON NULL AS IDENTITY MINVALUE 1 
         MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 
         NOORDER  NOCYCLE  NOKEEP  NOSCALE  NOT NULL ENABLE, 
	"FISHCOUNTERID" NUMBER, 
	"CONTAINERID" NUMBER, 
	"FISHCOUNTERSIZE" VARCHAR2(20), 
	 CONSTRAINT "FISHCOUNTER_PK" PRIMARY KEY ("IDENTCOUNTER")
  USING INDEX  ENABLE
   ) ;

/*FISHDESC_TABLE:*/
CREATE TABLE "FISHDESC_TABLE" 
   (	"IDENTFISH" NUMBER GENERATED BY DEFAULT ON NULL AS IDENTITY MINVALUE 1 
         MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 
         NOORDER  NOCYCLE  NOKEEP  NOSCALE  NOT NULL ENABLE, 
	"FISHID" NUMBER, 
	"FISHSIZE" VARCHAR2(20), 
	"LOCALEXPORT" VARCHAR2(20), 
	"FISHWEIGHT" VARCHAR2(20), 
	 CONSTRAINT "FISHDESC_PK" PRIMARY KEY ("IDENTFISH")
  USING INDEX  ENABLE
   ) ;

/*GPS_TABLE:*/
  CREATE TABLE "GPS_TABLE" 
   (	"IDENT" NUMBER GENERATED BY DEFAULT ON NULL AS IDENTITY MINVALUE 1 
         MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 
         NOORDER  NOCYCLE  NOKEEP  NOSCALE  NOT NULL ENABLE, 
	"GPSID" NUMBER, 
	"LONGITUDE" NUMBER(9,6), 
	"LATITUDE" NUMBER(9,6), 
	"TIMESTAMP" TIMESTAMP (6) WITH LOCAL TIME ZONE, 
	"TARGETOBJ" VARCHAR2(20), 
	"GPSORIGIN" VARCHAR2(20), 
	 CONSTRAINT "GPS_PK" PRIMARY KEY ("IDENT")
  USING INDEX  ENABLE
   ) ;

/*TEMPERATURE_TABLE:*/
  CREATE TABLE "TEMPERATURE_TABLE" 
   (	"IDENT" NUMBER GENERATED BY DEFAULT ON NULL AS IDENTITY MINVALUE 1 
         MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 
         NOORDER  NOCYCLE  NOKEEP  NOSCALE  NOT NULL ENABLE, 
	"TEMPID" NUMBER, 
	"TEMP" NUMBER, 
	"INRANGE" NUMBER, 
	"TIMESTAMPTEMP" TIMESTAMP (6) WITH TIME ZONE, 
	 CONSTRAINT "TEMPERATURE_TABLE_PK" PRIMARY KEY ("IDENT")
  USING INDEX  ENABLE
   ) ;

/*INSPECTOR_REPORT:*/
 CREATE TABLE "INSPECTOR_REPORT" 
   (	"INSPECTION_ID" NUMBER(8,0) GENERATED BY DEFAULT ON NULL AS IDENTITY MINVALUE 1 
         MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 
         NOORDER  NOCYCLE  NOKEEP  NOSCALE  NOT NULL ENABLE, 
	"FISH_TYPE" VARCHAR2(64 CHAR), 
	"FISH_SIZE" VARCHAR2(16 CHAR), 
	"QUALITY" VARCHAR2(16 CHAR), 
	"QUANTITY" NUMBER(6,0), 
	"FIRST_NAME" VARCHAR2(32), 
	"LAST_NAME" VARCHAR2(32), 
	"INSPECTION_DATE" DATE, 
	 CONSTRAINT "INSPECTOR_REPORT_PK" PRIMARY KEY ("INSPECTION_ID")
  USING INDEX  ENABLE
   ) ;


/*GET SQL_COMMANDS:*/

/*FISHCOUNTER_TABLE:*/
select * from FISHCOUNTER_TABLE order by IDENTCOUNTER desc

/*FISHDESC_TABLE:*/
select * from FISHDESC_TABLE order by IDENTFISH desc

/*GPS_TABLE:*/
select * from GPS_TABLE order by IDENT desc

/*TEMPERATURE_TABLE:*/
select * from TEMPERATURE_TABLE order by IDENT desc

/*INSPECTOR_REPORT:*/
select * from INSPECTOR_REPORT order by INSPECTION_ID desc


/*POST SQL_COMMANDS:*/

/*FISHCOUNTER_TABLE:*/
BEGIN

INSERT INTO FISHCOUNTER_TABLE (IDENTCOUNTER, FISHCOUNTERID, CONTAINERID, FISHCOUNTERSIZE)
VALUES (COUNTIDENT_INCR.nextval, FISHID_INCR.nextval,:containerId, :fishCounterSize);

END;

/*FISHDESC_TABLE:*/
BEGIN

INSERT INTO FISHDESC_TABLE (IDENTFISH, FISHID, FISHSIZE, LOCALEXPORT, FISHWEIGHT)
VALUES (IDENTFISH_INCR.nextval, FISHID_INCR.nextval, :fishSize, :LocalExport, :fishWeight );

END;

/*GPS_TABLE:*/
BEGIN

INSERT INTO GPS_TABLE (IDENT, GPSID,LONGITUDE, LATITUDE, TIMESTAMP,TARGETOBJ,GPSORIGIN)
VALUES (GPSIDENT_INCR.nextval, GPSID_INCR.nextval,:longi, :lati, TO_TIMESTAMP(:TimeStamp, 'YYYY-MM-DD HH24:MI:SS'),:targetOBJ,:gpsOrigin);

END;

/*TEMPERATURE_TABLE:*/
BEGIN

INSERT INTO TEMPERATURE_TABLE (IDENT, TEMPID, TEMP, INRANGE, TIMESTAMPTEMP)
VALUES (IDENT_INCR.nextval, TEMPID_INCR.nextval, :temp, :inRange, TO_TIMESTAMP(:timeStamps, 'YYYY-MM-DD HH24:MI:SS'));

END;

/*INSPECTOR_REPORT:*/

/*CREATE SEQUENCES SQL_COMMANDS:*/

/*COUNTIDENT_INCR:*/
CREATE SEQUENCE  "COUNTIDENT_INCR"  MINVALUE 1 MAXVALUE 9999999999 INCREMENT BY 1 START WITH 521 CACHE 20 NOORDER  NOCYCLE  NOKEEP  NOSCALE  GLOBAL ;

/*FISHID_INCR:*/
CREATE SEQUENCE  "FISHID_INCR"  MINVALUE 1 MAXVALUE 999999 INCREMENT BY 1 START WITH 541 CACHE 20 NOORDER  NOCYCLE  NOKEEP  NOSCALE  GLOBAL ;

/*GPSIDENT_INCR:*/
CREATE SEQUENCE  "GPSIDENT_INCR"  MINVALUE 10 MAXVALUE 999999 INCREMENT BY 1 START WITH 330 CACHE 20 NOORDER  NOCYCLE  NOKEEP  NOSCALE  GLOBAL ;

/*GPSID_INCR:*/
CREATE SEQUENCE  "GPSID_INCR"  MINVALUE 1 MAXVALUE 99999999 INCREMENT BY 1 START WITH 321 CACHE 20 NOORDER  NOCYCLE  NOKEEP  NOSCALE  GLOBAL ;

/*IDENTFISH_INCR:*/
CREATE SEQUENCE  "IDENTFISH_INCR"  MINVALUE 1 MAXVALUE 999999 INCREMENT BY 1 START WITH 50 CACHE 20 NOORDER  NOCYCLE  NOKEEP  NOSCALE  GLOBAL ;

/*IDENT_INCR:*/
CREATE SEQUENCE  "IDENT_INCR"  MINVALUE 5 MAXVALUE 99999999 INCREMENT BY 1 START WITH 925 CACHE 20 NOORDER  NOCYCLE  NOKEEP  NOSCALE  GLOBAL ;

/*INSPECTOR_ID_SEQ:*/
CREATE SEQUENCE  "INSPECTOR_ID_SEQ"  MINVALUE 1 MAXVALUE 99999999 INCREMENT BY 1 START WITH 41 CACHE 20 NOORDER  NOCYCLE  NOKEEP  NOSCALE  GLOBAL ;

/*TEMPID_INCR:*/
CREATE SEQUENCE  "TEMPID_INCR"  MINVALUE 5 MAXVALUE 9999999 INCREMENT BY 1 START WITH 925 CACHE 20 NOORDER  NOCYCLE  NOKEEP  NOSCALE  GLOBAL ;
